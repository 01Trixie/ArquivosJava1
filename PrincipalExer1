package view;

import java.io.IOException;

import controller.Exer1Controller;

public class PrincipalExer1 {
	
	public static void main(String[] args) {
		
		Exer1Controller operacao = new Exer1Controller();
		
		String caminho = "C:\\TEMP\\";
		String nome = "generic_food.csv";
		
		try {
			operacao.readFile(caminho, nome);
		} catch (IOException e) {
			e.printStackTrace();
		}
		

	}
	
	
}
